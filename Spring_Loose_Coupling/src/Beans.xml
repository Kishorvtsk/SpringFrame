<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mathCheatObjectValue" class="com.spring.Lc.MathCheat"></bean>
	<bean id="scienceCheatObjectValue"	class="com.spring.Lc.ScienceCheat"></bean>
	<bean id="javaCheatObjectValue"	class="com.spring.Lc.JavaCheat"></bean>

	<bean id="student" class="com.spring.Lc.Student">
		<property name="cheat" ref="javaCheatObjectValue" />
	</bean>




</beans>